<%%= form_for ([<%= namespace_controller.present? ? ":#{namespace_controller}, ":"" %>@<%= instance_name %>]) do |f| %>
<div id="error_explanation">
  <%%= f.error_messages %>
</div>
<%- for attribute in model_attributes -%>
<div class="field">
  <%%= f.label :<%=attribute.name %> %><br />
  <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
</div>
<%- end -%>
<div class="actions">
  <%%= f.submit %>
</div>
<%% end %>
